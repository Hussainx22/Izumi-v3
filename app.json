{
  "name": "Izumi-v3",
  "description": "Izumi a Multi Device WhatsApp bot",
  "logo": "https://i.imgur.com/A4IPfdI.jpeg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/sataniceypz/Izumi-v3",
  "website": "https://eypz-izumi.vercel.app",
  "success_url": "https://github.com/sataniceypz/Izumi-v3",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "izumi~earT3MQH",
      "required": true
    },
    "HEROKU": {
      "description": "Don't change this",
      "required": true,
      "value": "true"
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account API key, https://dashboard.heroku.com/account",
      "required": true
    },
    "MENU_URL": {
      "description": "Add URL of image or video",
      "required": false,
      "value": "https://i.imgur.com/A4IPfdI.jpeg"
    },
    "BOT_NAME": {
      "description": "Enter your bot name",
      "required": false,
      "value": "Iᴢᴜᴍɪ-ᴍᴅ"
    },
    "STICKER_PACKNAME": {
      "description": "Enter your sticker pack name",
      "required": false,
      "value": "Hᴜssᴀɪɴ 〆ᬊᬁ"
    },
    "AUDIO_DATA": {
      "description": "Enter your audio data (title, author, link)",
      "required": false,
      "value": "enjoy this song, Iᴢᴜᴍɪ-v3, https://i.imgur.com/A4IPfdI.jpeg"
    },
    "OWNER_NAME": {
      "description": "Enter your name",
      "required": false,
      "value": "Hᴜssᴀɪɴ 〆ᬊᬁ"
    },
    "CAPTION": {
      "description": "Enter your caption",
      "required": false,
      "value": ""
    },
    "PREFIX": {
      "description": "Prefix for bot",
      "required": false,
      "value": "."
    },
    "AUTHOR": {
      "description": "Enter sticker pack author name",
      "required": false,
      "value": "Hᴜssᴀɪɴ 〆ᬊᬁ"
    },
    "SUDO": {
      "description": "Add Sudo number",
      "required": false,
      "value": "8801334089349"
    },
    "MODE": {
      "description": "Bot work type",
      "required": true,
      "value": "public"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku-community/apt"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}
